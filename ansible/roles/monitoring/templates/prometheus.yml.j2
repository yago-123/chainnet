global:
  scrape_interval: 15s  # Set the default scrape interval to 15 seconds
  scrape_timeout: 10s   # (Optional) Timeout for scraping metrics

scrape_configs:
  - job_name: 'local_scrape'  # Name of the scrape job
    metrics_path: '/metrics'
    static_configs:
      - targets: ['127.0.0.1:9091']  # Target endpoint
